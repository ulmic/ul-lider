%footer
  %br
    .row
      .col-md-1.logo
        = link_to "http://ulmic.ru" do
          = image_tag 'main/logo-mic.png'
      .col-md-4
        &copy
        = copyright_notice_year_range 2013
        = t('contest.organization')
        %br
        = link_to 'http://ulmic.ru', 'http://ulmic.ru', target: '_blank'
        %br
        = link_to t('application.developers'), 'https://github.com/ulmic', target: '_blank'
        %br
        = link_to t('application.github_link'), 'https://github.com/ulmic/ul-lider', target: '_blank'
      .col-md-3
        = link_to t('.send_request'), new_user_path
        %br
        = link_to t('.news'), news_index_path
        %br
        = link_to t('contest.definition'), "/pages/the_competition"
      .col-md-3
        = link_to configus.chrome_extension.url, target: "_blank" do
          = image_tag 'main/availale_in_chrome_web_store.png', class: :store_logo
= stylesheet_link_tag "vendor", media: "all", debug: false
= stylesheet_link_tag "application"
= javascript_include_tag "vendor", debug: false
= javascript_include_tag "application"
= render 'layouts/web/shared/google_analytics'
