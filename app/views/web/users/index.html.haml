- model_class = User
- title_is 'Участники, прошедшие во II этап конкурса «Я-лидер!»'
.page-header
  %h2
    =  'Участники, прошедшие во II этап конкурса «Я-лидер!»'
  = render 'layouts/web/shared/social_buttons'
%table.table.table-striped.table-condensed
  %thead
    %tr
      %th= model_class.human_attribute_name(:avatar)
      %th= model_class.human_attribute_name(:full_name)
      %th= model_class.human_attribute_name(:place)
      %th= model_class.human_attribute_name(:average)
  %tbody
    - users.each do |user|
      %tr
        %td= image_tag user.avatar.small
        %td= link_to user.full_name, edit_admin_user_path(user)
        %td= user.place
        %td= user.average
= render 'layouts/web/shared/social_buttons'
